# alias kolekti="docker run -ti kolekti-cli kolekti"

kolekti() {

PROJECT=$1
shift

# echo \
docker run --rm \
           -v $PROJECT:/usr/lib/kolekti/project \
           -v $PROJECT/kolekti/fonts:/usr/share/fonts/extra \
           -v $PROJECT/kolekti/princexml:/usr/lib/prince/license \
           -ti \
           --entrypoint "kolekti" \
           kolekti-cli -b /usr/lib/kolekti/project $@
}